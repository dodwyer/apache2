<VirtualHost *:8080>
  ServerAdmin <%= node['apache']['contact'] %>

  DocumentRoot <%= node['apache']['docroot_dir'] %>/

  ErrorLog <%= node['apache']['log_dir'] %>/<%= node['apache']['error_log'] %>

  LogLevel warn

  CustomLog <%= node['apache']['log_dir'] %>/<%= node['apache']['access_log'] %> combined

</VirtualHost>
